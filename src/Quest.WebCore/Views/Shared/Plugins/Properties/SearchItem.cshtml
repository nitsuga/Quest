@model dynamic


<div style="color: white; width: 100%; height: 20px; margin: 5px">
    <!-- The model should resemble a standard QuestResource but as a dynamic object -->
    <p>.</p>
    <p></p>
    <p></p>
    <p></p>
    <div>
        <p>Resource @Model["d"].Value</p>
        <p><a target="_blank" href='@Model["url"]'>@Model["url"]</a></p>
        <p>Status @Model["v"].Value</p>
        <div>
            <video controls autoplay name="media"><source src="@Model["v"] " type="video/mp4"></video>
        </div>
        <div><image src='@Model["i"]'/></div>
    </div>
</div>
